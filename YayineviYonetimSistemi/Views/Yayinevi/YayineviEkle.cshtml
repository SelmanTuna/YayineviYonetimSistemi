@model YayineviYonetimSistemi.Models.Entity.Yayinevleri
@{
    ViewBag.Title = "YayineviEkle";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<form class="form-group" method="post">
    <div></div>
    <div style="margin-top:15px">
        <label>Yayınevi Adı</label>
       @Html.TextBoxFor(y=>y.yayineviAdi,new {@class="form-control"})
    </div>
    <div style="margin-top:15px">
        <button class="btn btn-info" id="Btn1">Yayınevi Ekle</button>
    </div>
</form>


